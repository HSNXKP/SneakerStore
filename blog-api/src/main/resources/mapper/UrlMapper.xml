<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.naccl.mapper.UrlMapper">

    <resultMap id="BaseResultMap" type="top.naccl.entity.Url">
        <id property="id" column="id"/>
        <result property="url" column="url"/>
    </resultMap>

    <select id="getAllUrl"  resultMap="BaseResultMap">
        select u.id,u.url from url as u
    </select>


</mapper>